platform :ios, '8.0'
target 'Utrust' do
use_frameworks!
pod 'Masonry'        #自动布局，全局使用
pod 'ReactiveCocoa', '~> 2.5'    #编程框架，全局使用
pod 'AFNetworking'    #网络请求
pod 'IQKeyboardManager'  #键盘辅助
pod 'MBProgressHUD'  #tip提示
pod 'SDWebImage'     #异步加载图片
pod 'pop', '1.0.7'              #facebook动画库
pod 'MJRefresh'    #下拉刷新，上拉加载更多
pod 'WebViewJavascriptBridge' #js webView互相调用
pod 'MJExtension'  #字典 模型 互转
pod 'SDCycleScrollView'
pod 'XMPPFramework', :git => "https://github.com/robbiehanson/XMPPFramework.git", :branch => 'master'

end
